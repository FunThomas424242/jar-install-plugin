<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<groupId>com.github.funthomas424242</groupId>
	<artifactId>jar-install-plugin</artifactId>
	<packaging>maven-plugin</packaging>
	<version>0.0.3-SNAPSHOT</version>
	<name>JAR Install Plugin</name>
	<inceptionYear>2010</inceptionYear>

	<url>https://github.com/FunThomas424242/jar-install-plugin/</url>

	<description>
		Maven plugin to download jar files and install them as artifact into
	  the local maven repository. Additional the artifacts will be added to 
	  the dependency list of maven project.
	</description>


	<prerequisites>
		<maven>3.3.9</maven>
	</prerequisites>


	<properties>
		<project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
		<java.version>1.8</java.version>
	</properties>

	<profiles>
		<profile>
			<id>local</id>
			<build>
				<plugins>
					<plugin>
						<groupId>org.apache.maven.plugins</groupId>
						<artifactId>maven-assembly-plugin</artifactId>
						<version>2.2-beta-5</version>
						<executions>
							<execution>
								<id>stdassembly</id>
								<phase>package</phase>
								<goals>
									<goal>single</goal>
								</goals>
								<configuration>
									<tarLongFileMode>gnu</tarLongFileMode>
									<outputDirectory>${project.build.directory}/archives</outputDirectory>
									<descriptorRefs>
										<descriptorRef>src</descriptorRef>
										<descriptorRef>project</descriptorRef>
										<descriptorRef>bin</descriptorRef>
										<descriptorRef>jar-with-dependencies</descriptorRef>
									</descriptorRefs>
								</configuration>
							</execution>
							<execution>
								<id>siteassembly</id>
								<phase>package</phase>
								<goals>
									<goal>single</goal>
								</goals>
								<configuration>
									<outputDirectory>${project.build.directory}/archives</outputDirectory>
									<tarLongFileMode>gnu</tarLongFileMode>
									<descriptors>
										<descriptor>src/main/assembly/site.xml</descriptor>
									</descriptors>
								</configuration>
							</execution>
						</executions>
					</plugin>
				</plugins>
			</build>
		</profile>
		<profile>
			<id>remote</id>
			<build>
				<plugins>

				</plugins>
			</build>
		</profile>
	</profiles>

	<scm>
		<url>https://github.com/FunThomas424242/jar-install-plugin</url>
		<connection>scm:git:https://github.com/FunThomas424242/jar-install-plugin.git</connection>
